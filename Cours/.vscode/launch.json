{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Listen for Xdebug (9003)",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "hostname": "0.0.0.0",
      "pathMappings": {
        "/workspace": "${workspaceFolder}"
      },
      "xdebugSettings": {
        "max_children": 256,
        "max_data": 1024,
        "max_depth": 5
      }
    }
  ]
}

